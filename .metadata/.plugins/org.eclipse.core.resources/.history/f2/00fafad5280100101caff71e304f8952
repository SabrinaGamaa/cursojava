
public class condicionais {
	
	public static void main(String[] args) {
		
		int x = 7;
		int y = 9;
		
		System.out.println(x > y);
		System.out.println(x < y);
		System.out.println(x >= y);
		System.out.println(x <= y);
		System.out.println(x != y);
		System.out.println(x == y);
		System.out.println(x < y && x != y);
//		OR
//		AND
		
	}
}
