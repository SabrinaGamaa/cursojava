package exercicios;

import java.util.Locale;
import java.util.Scanner;

public class ex001 {
	
	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		System.out.print("Quantos números você vai digitar? ");
		int num = sc.nextInt();

		
		sc.close();
		
	}
}
