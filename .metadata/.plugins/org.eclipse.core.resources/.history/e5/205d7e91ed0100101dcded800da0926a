import java.util.Locale;

public class split {

	public static void main(String[] args) {
		String s = "potato aplle lemon";
		
//		Como se fosse a lista em python
		String[] vect = s.split(" ");
//		String word1 = vect[0];
//		String word2 = vect[1];
//		String word3 = vect[2];
		
		for (int i=0; i<3; i++) {
			System.out.println(vect[i]);
		}
	}

}

//%f - ponto flutuante
//%d - numero inteiro
//%s - String
//\n - quebra de linha
