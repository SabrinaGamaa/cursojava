package exercicios;

import java.util.Locale;
import java.util.Scanner;

public class ex001 {
	
	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int num = sc.nextInt();
		System.out.printf("Quantos números você vai digitar? %d\n", num);

		
		sc.close();
		
	}
}
