
public class funcoespstring {

	public static void main(String[] args) {
		
		String original = "adeDFEW WEFdewf FWWFf rfref";
		
		String minuscula = original.toLowerCase();
		
		System.out.println("Texto original: " + original);
		System.out.println("Texto com tudo minusculo: " + minuscula);
		
		
	}
}
