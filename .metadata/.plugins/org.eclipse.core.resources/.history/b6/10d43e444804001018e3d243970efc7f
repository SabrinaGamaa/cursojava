package entities;

public class Account {
	private String name;
	private int number;
	private double balance;
	
	public Account(String name, int number, double balance) {
		this.name = name;
		this.number = number;
		this.balance = balance;
	}
	
	public Account(String name, int number) {
		this.name = name;
		this.number = number;
	}
	
	
	
}
